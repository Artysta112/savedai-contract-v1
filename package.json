{
  "name": "savedai-contract-v1",
  "version": "1.0.0",
  "description": "Smart contracts for the saveDAI dapp",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
